
SHELL = /bin/sh

TEXHOME = /salsify/packages/teTeX/bin/
TEXHOME = /usr/bin/

#all: kcarta1.22.ps bkcarta1.10.ps ReadMeFirst.ps
all:kcarta1.22.pdf bkcarta1.10.pdf ReadMeFirst.pdf

kcarta1.22.pdf: kcarta1.22.tex ASL_defs.tex
	$(TEXHOME)latex kcarta1.22.tex && \
	$(TEXHOME)latex kcarta1.22.tex && \
	$(TEXHOME)dvips -f kcarta1.22.dvi > kcarta1.22.pdf

